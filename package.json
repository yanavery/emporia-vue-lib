{
  "name": "emporia-vue-lib",
  "version": "1.0.0",
  "description": "Unofficial TypeScript library for interacting with the Emporia Vue energy monitoring system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "emporia",
    "vue",
    "energy",
    "monitoring",
    "typescript",
    "home-automation"
  ],
  "author": "Yan Avery, ported from PyEmVue by magico13",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yanavery/emporia-vue-lib"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "simulator": "ts-node src/simulator/main.ts"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "amazon-cognito-identity-js": "^6.3.15",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "fastify": "^5.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "test-exclude": "7.0.1"
  }
}